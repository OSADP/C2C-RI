		<operation name="OP_RIXMLFtpDMSInventory">
			<ftp:operation location="RIDMSInventory.zip"/>
			<input>
				<ftp:urlEncoded/>
			</input>
			<output>
				<mime:content type="gzip"/>
			</output>
		</operation>
	</binding>


