<script language="javascript">
    function showHide(ind, imageId) {
      var image = document.getElementById(imageId);
      var imageInd = image.src.substring(image.src.lastIndexOf(".") -1, image.src.lastIndexOf(".") );
      image.src = image.src.substring(0, image.src.lastIndexOf(".") - 1) + ((imageInd.indexOf("0") != -1)?"1":"0") +".jpg";
      var trE = document.getElementById(ind);
      if(trE.className == 'hide') {
        trE.className = 'show';
      }else {
        trE.className = 'hide';
      }
    }
</script>
